{
  "version": 2,
  "builds": [
    {
      "src": "template/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    },
    {
      "src": "assests/**",
      "use": "@vercel/static"
    },
    {
      "src": "UMAIYAVAN_RESUME.pdf",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/assests/(.*)",
      "dest": "/assests/$1"
    },
    {
      "src": "/UMAIYAVAN_RESUME.pdf",
      "dest": "/UMAIYAVAN_RESUME.pdf"
    },
    {
      "src": "/(.*)",
      "dest": "/template/index.html"
    }
  ]
}
